{
  "bitwise": true,
  "browser": true,
  "camelcase": false,
  "curly": true,
  "esnext": true,
  "eqeqeq": true,
  "eqnull": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxlen": false,
  "maxstatements": 50,
  "newcap": true,
  "node": true,
  "strict": true,
  "trailing": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "white": true,
  "predef": [
    "alert",
    "console",
    "setTimeout",
    "setInterval",
    "require",
    "define",
    "module"
  ]
}
